function getCount(str) {
  let counter = 0
  for(let i = 0;i < str.length; i++){
    if(str[i]== 'a'){
      counter += 1
    }else if(str[i]== 'e'){
      counter += 1
    }else if(str[i]== 'i'){
      counter += 1
    }else if(str[i]== 'o'){
      counter += 1
    }else if(str[i]== 'u'){
      counter += 1
    }
  }
  return counter
}