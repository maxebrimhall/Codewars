function removeSmallest(numbers) {
  let arr = []
  let repeatArr = []
  let result = Math.min(...numbers)
  for(let i = 0; i < numbers.length; i++){
    if(numbers[i] !== result){
      arr.push(numbers[i])
    }else if(repeatArr > 0){
      arr.push(numbers[i])
    }else if(numbers[i] == result){
      repeatArr.push(numbers[i])
    }
  }
  return arr;
}